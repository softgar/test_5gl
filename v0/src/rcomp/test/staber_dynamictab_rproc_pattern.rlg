	OBJ_form = {
        type: 'assembly',
        children: [
            {
                type: dynamictab,
                elements: [
                    {
                        label: {
                            d: 'Pestaña 1'
                        },
                        actions: [
                            {
                                label: {
                                    icon: 'cancel'
                                },
                                collapse: false
                            },
                            {
                                label: {
                                    d: 'Acción 1'
                                },
                                collapse: true,
                                onclick: 'alert( \"Acción 1 pulsada\")'
                            }
                        ]
                    },
                    {
                        label: {
                            d: 'Pestaña 2'
                        },
                        actions: [
                            {
                                label: {
                                    icon: cancel
                                },
                                collapse: false
                            }
                        ]
                    },
                    {
                        label: {
                            d: 'Pestaña 3'
                        },
                        actions: [
                            {
                                label: {
                                    icon: cancel
                                },
                                collapse: false
                            }
                        ]
                    }
                ]
            }
        ]
	},
    Rproc = {
        type: 'rprocess',
        gui_out: OBJ_form,
        hasKw: rproc__4gl_dynamictab__001,
        hasDsca: [(es, 'Test_001 dynamictab')],
        hasIcon: 'dinamic',
        isRoot: false,
        security_edit: [permissionProfile__repositorioTests_KRP],
        security_view: [permissionProfile__repositorioTests_KRP]

    },
