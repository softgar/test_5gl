'staber_password_rproc_pattern::folders'(_, _, _, _):-
	Conf_folder = {
        type: 'krp:folder',
        hasKw: folder__repositorioTests_rproc_passwordhard,
        hasDsca: 'Tests passwords',
        hasIcon: 'key',
        isRoot: false,
        security_edit: [permissionProfile__repositorioTests_KRP],
        security_view: [permissionProfile__repositorioTests_KRP],
        hasERCPart: [
            rproc__4gl_password__001,
            rproc__4gl_passwordhard__002,
            rproc__4gl_passwordinputtype__003
        ]
	},
    create_rprocess(_,Conf_folder,_,_).