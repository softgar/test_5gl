setConfigureOutput5GLAsConsoleTest(Obj_5GL, ProcToConfig, CHANNEL):-
    newKw('busid__', BUS_CONSOLE),
    Obj_console = {      
        in: {},
        type: 'rprocess',  
        busId: BUS_CONSOLE,    
        evtSubscribe: [{topic:'setNode',channel: CHANNEL}],
        gui_out: { type: 'method', name: 'console_out', module: 'GenericUI'}
    },
    setStmtj('hasConfig',ProcToConfig,
    {
        gui_out:{
            type: 'assembly',
            children : [
                {
                    type:'assembly',
                    children: [
                        {
                            type: 'assembly',
                            width: 70,
                            children:[Obj_5GL]
                        },
                        {
                            type: 'assembly',
                            width: 30,
                            children:[Obj_console]
                        }
                    ]
                }                
            ]
        }                    
    }). 